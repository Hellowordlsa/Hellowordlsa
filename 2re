VG.Protect = function(Gui)
    if type(Gui) == "userdata" then
        if sethiddenproperty then
            sethiddenproperty(Gui, "RobloxLocked", true)
        elseif not sethiddenproperty then
            Gui.RobloxLocked = true
        end
    end
    Gui.Parent = VG.GetProtecter()
end

